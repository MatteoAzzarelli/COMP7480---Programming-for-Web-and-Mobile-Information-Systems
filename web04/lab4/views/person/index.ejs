<!--index.ejs-->
<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    <% persons.forEach( function(model) { %>
        <tr>
            <td>
                <%= model.id %>
            </td>
            <td>
                <%= model.name %>
            </td>
            <td>
                <%= model.age %>
            </td>
        </tr>
        <% }); %>
</table>